# sorting with numbers asc
{
    var list1 = [3, 4, 1, 5, 2, 8, 4]
    $list1.sort()
    var idx = 0
    var nx = 1
    loop $nx < $list1.len() {
        assert $list1[$idx] <= $list1[$nx]
        $nx += 1
        $idx += 1
    }
}

