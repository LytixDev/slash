var fib_iterative = n {
    var a = 0
    var b = 1
    loop _ in 1..$n+1 {
        var c = $a + $b
        $a, $b = $b, $c
    }
    return $a
}

var fib_recursive = n {
    $n <= 1 && return $n
    return $fib_recursive($n - 1) + $fib_recursive($n - 2)
}

assert $fib_iterative(20) == 6_765
assert $fib_recursive(20) == 6_765
